.navbottom {
  position: fixed;
  bottom: 0px;
  height: 54px;
  line-height: 50px;
  overflow: hidden;
  width: 100%;
  background-color: white;
}

.navbottom .navitem {
  float: left;
  width: 50%;
  display: block;
  height: 50px;
  line-height: 25px;
  text-align: center;
  padding-top: 4px;
}

.icon_apply {
  margin: 0px auto;
  width: 25px;
  height: 25px;
  background: url(../images/icon_apply.png) no-repeat;
  background-size: cover;
}

.on .icon_apply {
  margin: 0px auto;
  width: 25px;
  height: 25px;
  background: url(../images/icon_apply2.png) no-repeat;
  background-size: cover;
}

.icon_my {
  margin: 0px auto;
  width: 25px;
  height: 25px;
  background: url(../images/icon_my.png) no-repeat;
  background-size: cover;
}

.on .icon_my {
  margin: 0px auto;
  width: 25px;
  height: 25px;
  background: url(../images/icon_my2.png) no-repeat;
  background-size: cover;
}

.text_apply {
  color: #929292;
}

.on .text_apply {
  color: #66CCCC;
}

