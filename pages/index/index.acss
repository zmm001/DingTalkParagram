.iconimg {
  width: 80px;
  height: 80px;
  border-radius: 50%;
  border: 3px solid ccc;
}

.loginout {
  margin: 0px auto;
  margin-top: 30px;
  height: 40px;
  line-height: 40px;
  width: 80%;
  text-align: center;
  border-radius: 5px;
  color: white;
  background: #ff7f7f
}

view {
  box-sizing: border-box;
}

.item-box {
  width: 700rpx;
  margin: 0 auto;
}

.items {
  width: 100%;
  margin-top: 10px;
}

.item {
  position: relative;
  /* border-top: 2rpx solid #eee; */
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
  border-bottom: 1px solid #E6E6E6;
}

.item:last-child {
  border-bottom: 2rpx solid #eee;
}

.inner {
  position: absolute;
  top: 0;
}

.inner.txt {
  background-color: #fff;
  width: 100%;
  z-index: 5;
  padding: 0 10rpx;
  transition: left 0.2s ease-in-out;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.inner.del {
  background-color: #e64340;
  width: 180rpx;
  text-align: center;
  z-index: 4;
  right: 0;
  color: #fff;
}

.item-icon {
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  margin-right: 16rpx;
  margin-left: 6px;
  border-radius: 50%;
}

.item-icons {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-left: 6px;
  border-radius: 50%;
  display: inline-block;
}
.text-aglin-right{
  text-align: right;
}

.item-data {
  /* margin-left: 15%; */
  width: 53%;
}

